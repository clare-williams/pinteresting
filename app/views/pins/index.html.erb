 <div id="pins">
  <% @pins.each do |pin| %>
  <div class="box">
     
        <%= link_to (image_tag pin.image.url(:medium)), pin %><br /><%= pin.description %><br/>
       <%= pin.user.name %> <br />
        <% if pin.user == current_user %>
	     	<%= link_to 'Edit', edit_pin_path(pin) %>
	      	<%= link_to 'Destroy', pin, method: :delete, data: { confirm: 'Are you sure?' } %><br/><br /><br/><br/><br /><br/>
      <% end %>
    <% end %>
  

<br/>
<br/>

<%= link_to 'New Pin', new_pin_path %>
<br/>
</div>
</div>